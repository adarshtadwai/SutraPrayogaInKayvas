{
  "text": "raghuvansham",
  "title": "रघुवंशम्",
  "base_link": "https://sanskritsahitya.org/",
  "comment": "This file contains 446 unique Panini sutras referenced in the commentary (In-progress sample: first 50 verses)",
  "data": [
    {
      "loc": "1.1",
      "v": "वागर्थाविव संपृक्तौ वागर्थप्रतिपत्तये । जगतः पितरौ वन्दे पार्वतीपरमेश्वरौ ॥",
      "sutra_sentences": [
        {
          "sutra": "1.2.70",
          "word": "",
          "sentence": "माता च पिता च पितरौ, `पिता मात्रा` इति द्वन्द्वैकशेषः।"
        },
        {
          "sutra": "4.1.92",
          "word": "",
          "sentence": "`तस्यापत्यम्` इच्यण्"
        },
        {
          "sutra": "4.1.15",
          "word": "",
          "sentence": "`टिङ्ढाणञ्-` इत्यादिना ङीप्।"
        }
      ]
    },
    {
      "loc": "1.2",
      "v": "क्व सूर्यप्रभवो वंशः क्व चाल्पविषया मतिः । तितीर्षुर्दुस्तरं मोहादुडुपेनास्मि सागरम् ॥",
      "sutra_sentences": [
        {
          "sutra": "3.3.57",
          "word": "",
          "sentence": "`ॠदोरप्` ।"
        },
        {
          "sutra": "3.3.19",
          "word": "",
          "sentence": "`अकर्तरि च कारके संज्ञायाम्` इति साधुः।"
        },
        {
          "sutra": "3.3.126",
          "word": "",
          "sentence": "`ईषद्दुःसुषु-` इत्यादिना खल्प्रत्ययः।"
        }
      ]
    },
    {
      "loc": "1.3",
      "v": "मन्दः कवियशःप्रार्थी गमिष्याम्युपहास्यताम् । प्रांशुलभ्ये फले लोभादुद्बाहुरिव वामनः ॥",
      "sutra_sentences": [
        {
          "sutra": "3.1.124",
          "word": "",
          "sentence": "`ऋहलोर्ण्यत्` इति ण्यत्प्रत्ययः।"
        }
      ]
    },
    {
      "loc": "1.5",
      "v": "सोऽहमाजन्मशुद्धानामाफलोदयकर्मणाम् । आसमुद्रक्षितीशानामानाकरथवर्त्मनाम् ॥",
      "sutra_sentences": [
        {
          "sutra": "2.1.13",
          "word": "",
          "sentence": "`आङ् मर्यादाभिविध्योः` इत्यव्ययीभावः, तस्य शुद्धानामित्यनेन सुप्सुपेति समासः।"
        }
      ]
    },
    {
      "loc": "1.6",
      "v": "यथाविधिहुताग्नीनां यथाकामार्चितार्थिनाम् । यथापराधदण्डानां यथाकालप्रबोधिनाम् ॥",
      "sutra_sentences": [
        {
          "sutra": "2.1.7",
          "word": "",
          "sentence": "`यथासादृश्ये` इत्यव्ययीभावः।"
        }
      ]
    },
    {
      "loc": "1.7",
      "v": "त्यागाय संभृतार्थानां सत्याय मितभाषिणाम् । यशसे विजिगीषूणां प्रजायै गृहमेधिनाम् ॥",
      "sutra_sentences": [
        {
          "sutra": "2.1.36",
          "word": "",
          "sentence": "अत्र `त्यागाय` इत्यादिषु `चतुर्थी तदर्थ-` इत्यादिना तादर्थ्ये चतुर्थी।"
        }
      ]
    },
    {
      "loc": "1.8",
      "v": "शैशवेऽभ्यस्तविद्यानां यौवने विषयैषिणाम् । वार्धके मुनिवृत्तीनां योगेनान्ते तनुत्यजाम् ॥",
      "sutra_sentences": [
        {
          "sutra": "5.1.129",
          "word": "",
          "sentence": "`प्राणभृज्जातिवयोवचनोद्गात्र-` इत्यञ्प्रत्ययः।"
        },
        {
          "sutra": "5.1.133",
          "word": "",
          "sentence": "`द्वन्द्वमनोज्ञादिभ्यश्च` इति वुञ्प्रत्ययः।"
        },
        {
          "sutra": "3.2.178",
          "word": "",
          "sentence": "`अन्येभ्योऽपि दृश्यते` इति क्विप्।"
        }
      ]
    },
    {
      "loc": "1.12",
      "v": "तदन्वये शुद्धिमति प्रसूतः शुद्धिमत्तरः । दिलीप इति राजेन्दुरिन्दुः क्षीरनिधाविव ॥",
      "sutra_sentences": [
        {
          "sutra": "5.3.57",
          "word": "",
          "sentence": "`द्विवचनविभज्योप-` इत्यादिना तरप्प्रत्ययः।"
        }
      ]
    },
    {
      "loc": "1.13",
      "v": "व्यूढोरस्को वृषस्कन्धः शालप्रांशुर्महाभुजः । आत्मकर्मक्षमं देहं क्षात्रो धर्म इवाश्रितः ॥",
      "sutra_sentences": [
        {
          "sutra": "5.4.151",
          "word": "",
          "sentence": "`उरःप्रभृतिभ्यः कप्‌` ।"
        }
      ]
    },
    {
      "loc": "1.17",
      "v": "रेखामात्रमपि क्षुण्णादा मनोर्वर्त्मनः परम् । न व्यतीयुः प्रजास्तस्य नियन्तुर्नेमिवृत्तयः ॥",
      "sutra_sentences": [
        {
          "sutra": "5.2.37",
          "word": "",
          "sentence": "`प्रमाणे द्वयसच्-` इत्यादिना मात्रच्प्रत्ययः।"
        }
      ]
    },
    {
      "loc": "1.19",
      "v": "सेना परिच्छदस्तस्य द्वयमेवार्थसाधनम् । शास्त्रेष्वकुण्ठिता बुद्धिर्मौर्वी धनुषि चातता ॥",
      "sutra_sentences": [
        {
          "sutra": "3.3.118",
          "word": "",
          "sentence": "`पुंसि संज्ञायां घः प्रायेण` इति घप्रत्ययः।"
        },
        {
          "sutra": "6.4.96",
          "word": "",
          "sentence": "`छादेर्घोऽव्द्युपसर्गस्य` इत्युपधाह्रस्वः।"
        }
      ]
    },
    {
      "loc": "1.20",
      "v": "तस्य संवृतमन्त्रस्य गूढाकारेङ्गितस्य च । फलानुमेयाः प्रारम्भाः संस्काराः प्राक्तना इव ॥",
      "sutra_sentences": [
        {
          "sutra": "4.3.23",
          "word": "",
          "sentence": "`सायंचिरं-` इत्यादिना ट्युल्प्रत्ययः।"
        }
      ]
    },
    {
      "loc": "1.21",
      "v": "जुगोपात्मानमत्रस्तो भेजे धर्ममनातुरः । अगृध्नुराददे सोऽर्थमसक्तः सुखमन्वभूत् ॥",
      "sutra_sentences": [
        {
          "sutra": "3.2.140",
          "word": "",
          "sentence": "`त्रसिगृधिधृषिक्षिपेः क्नुः` इति क्नुप्रत्ययः।"
        }
      ]
    },
    {
      "loc": "1.23",
      "v": "अनाकृष्टस्य विषयैर्विद्यानां पारदृश्वनः । तस्य धर्मरतेरासीद्वृद्धत्वं जरसा विना ॥",
      "sutra_sentences": [
        {
          "sutra": "7.2.101",
          "word": "",
          "sentence": "`जराया जरसन्यतरस्याम्` इति जरसादेशः।"
        }
      ]
    },
    {
      "loc": "1.24",
      "v": "प्रजानां विनयाधानाद्रक्षणाद्भरणादपि । स पिता पितरत्त्तसां केवलं जन्महेतवः ॥",
      "sutra_sentences": [
        {
          "sutra": "3.2.99",
          "word": "",
          "sentence": "`उपसर्गे च संज्ञायाम्` इति डप्रत्ययः।"
        }
      ]
    },
    {
      "loc": "1.30",
      "v": "स वेलावप्रवलयां परिखीकृतसागराम् । अनन्यशासनामुर्वीं शशासैकपुरीमिव ॥",
      "sutra_sentences": [
        {
          "sutra": "3.2.101",
          "word": "",
          "sentence": "`अन्येष्वपि दृश्यते` इत्यत्र `अपि` शब्दात् खनेर्डप्रत्ययः।"
        }
      ]
    },
    {
      "loc": "1.31",
      "v": "तस्य दाक्षिण्यरूढेन नाम्ना मगधवंशजा । पत्नी सुदक्षिणेत्यादीदध्वरस्येव दक्षिणा ॥",
      "sutra_sentences": [
        {
          "sutra": "3.2.97",
          "word": "",
          "sentence": "`सप्तम्यां जनेर्डः` इति डप्रत्ययः।"
        }
      ]
    },
    {
      "loc": "1.35",
      "v": "अथाभ्यर्च्य विधातारं प्रयतौ पुत्रकाम्यया । तौ दंपती वसिष्ठस्य गुरोर्जग्मतुराश्रमम् ॥",
      "sutra_sentences": [
        {
          "sutra": "3.1.9",
          "word": "",
          "sentence": "`काम्यञ्च` इति `पुत्र` शब्दात् काम्यच्प्रत्ययः।"
        },
        {
          "sutra": "3.3.102",
          "word": "",
          "sentence": "`अ प्रत्ययात्` इति पुत्रकाम्यधातोरकारप्रत्ययः।"
        }
      ]
    },
    {
      "loc": "1.36",
      "v": "स्निग्धगम्भीरनिर्घोषमेकं स्यन्दनमाश्रितौ । प्रावृषेण्यं पयोवाहं विद्युदैरावताविव ॥",
      "sutra_sentences": [
        {
          "sutra": "4.3.17",
          "word": "",
          "sentence": "`प्रावृष एण्यः` इत्येण्यप्रत्ययः।"
        }
      ]
    },
    {
      "loc": "1.37",
      "v": "मा भूदाश्रमपीडेति परिमेयपुरः सरौ । अनुभावविशेषात्तु सेनापरिवृताविव ॥",
      "sutra_sentences": [
        {
          "sutra": "3.3.175",
          "word": "",
          "sentence": "`माङि लुङ्` इत्याशीरर्थे लुङ्।"
        },
        {
          "sutra": "6.4.74",
          "word": "",
          "sentence": "`न माङ्योगे` इत्यडागमनिषेधः।"
        }
      ]
    },
    {
      "loc": "1.38",
      "v": "सेव्यमानौ सुखस्पर्शैः शालनिर्यासगन्धिभिः । पुष्परेणूत्किरैर्वातैराधूतवनराजिभिः ॥",
      "sutra_sentences": [
        {
          "sutra": "3.1.135",
          "word": "",
          "sentence": "`इगुपध-` इत्यादिना किरतेः कप्रत्ययः।"
        }
      ]
    },
    {
      "loc": "1.40",
      "v": "परस्पराक्षिसादृश्यमदूरोज्झितवर्त्मसु । मृगद्वन्द्वेषु पश्यन्तौ स्यन्दनाबद्धदृष्टिषु ॥",
      "sutra_sentences": [
        {
          "sutra": "1.2.67",
          "word": "",
          "sentence": "`पुमान्स्त्रिया` इत्येकशेषः।"
        }
      ]
    },
    {
      "loc": "1.44",
      "v": "ग्रामेष्वात्मविसृष्टेषु यूपचिह्नेषु यज्वनाम् । अमोघाः प्रतिगृह्णन्तावर्ध्यानुपदमाशिषः ॥",
      "sutra_sentences": [
        {
          "sutra": "3.2.103",
          "word": "",
          "sentence": "`सुयजोर्ड्वनिप्` इति ङ्वनिप्र्रत्ययः।"
        },
        {
          "sutra": "5.4.25",
          "word": "",
          "sentence": "तदर्थं द्रव्यमर्ध्यम्। `पादार्धाभ्यां च` इति यत्प्रत्ययः।"
        }
      ]
    },
    {
      "loc": "1.45",
      "v": "हैयंगवीनमादाय घोषवृद्धानुपस्थितान् । नामधेयानि पृच्छन्तौ वन्यानां मार्गशाखिनाम् ॥",
      "sutra_sentences": [
        {
          "sutra": "2.2.23",
          "word": "",
          "sentence": "`हैयंगवीनं संज्ञायाम्` इति निपातः।"
        }
      ]
    },
    {
      "loc": "1.46",
      "v": "काप्यभिख्या तयोरासीद्व्रजतोः शुद्धवेषयोः । हिमनिर्मुक्तयोर्योगे चित्राचन्द्रमसोरिव ॥",
      "sutra_sentences": [
        {
          "sutra": "3.3.106",
          "word": "",
          "sentence": "`आतश्चोपसर्गे` इत्यङ्प्रत्ययः।"
        }
      ]
    },
    {
      "loc": "1.48",
      "v": "स दुष्प्रापयशाः प्रापदाश्रमं श्रान्तवाहनः । सायं संयमिनस्तस्य महर्षेर्महिषीसखः ॥",
      "sutra_sentences": [
        {
          "sutra": "5.4.91",
          "word": "",
          "sentence": "`राजाहःसखिभ्यष्टच` इति टच्प्रत्ययः।"
        }
      ]
    },
    {
      "loc": "1.49",
      "v": "वनान्तरादुपावृत्तैः समित्कुशफलाहरैः । पूर्यमाणमदृश्याग्रिप्रत्युद्यातैस्तपस्विभिः ॥",
      "sutra_sentences": [
        {
          "sutra": "3.2.11",
          "word": "",
          "sentence": "`आङिताच्छील्ये` इति हरतेराङ्पूर्वादच्प्रत्ययः।"
        }
      ]
    },
    {
      "loc": "1.54",
      "v": "अथ यन्तारमादिश्य धुर्यान्विश्रामयेति सः । तामवारोपयत्पत्नीं रथादवततार च ॥",
      "sutra_sentences": [
        {
          "sutra": "4.4.77",
          "word": "",
          "sentence": "`धुरो यड्ढकौ` इति यत्प्रत्ययः।"
        },
        {
          "sutra": "6.4.92",
          "word": "",
          "sentence": "दीर्घपाठे `मितां ह्रस्वः` इति सूत्रे वा `चित्तविरागे` इत्यतो `वा`इत्यनुवर्त्य व्यवस्थितविभाषाश्रयणत्वाद्ध्रस्वाभाव इति वृत्तकारः ॥"
        }
      ]
    },
    {
      "loc": "1.55",
      "v": "तस्मै सभ्याः सभार्याय गोप्त्रे गुप्ततमेन्द्रियाः । अर्हणामर्हते चक्रुर्मुनयो नयचक्षुषे ॥",
      "sutra_sentences": [
        {
          "sutra": "4.4.104",
          "word": "",
          "sentence": "`सभाया यः` इति यप्रत्ययः।"
        },
        {
          "sutra": "3.2.133",
          "word": "",
          "sentence": "`अर्हः प्रशंसायाम्` इति शतृप्रत्ययः।"
        }
      ]
    },
    {
      "loc": "1.56",
      "v": "विधेः सायंतनस्यान्ते स ददर्श तपोनिधिम् । अन्वासितमरुन्धत्या स्वाहयेव हविर्भुजम् ॥",
      "sutra_sentences": [
        {
          "sutra": "4.3.23",
          "word": "",
          "sentence": "`सायंचिरम्-` इत्यादिना ट्युल्प्रत्ययः।"
        }
      ]
    },
    {
      "loc": "1.58",
      "v": "तमातिथ्यक्रियाशान्तरथक्षोभपरिश्रमम् । पप्रच्छ कुशलं राज्ये राज्याश्रममुनिं मुनिः ॥",
      "sutra_sentences": [
        {
          "sutra": "5.4.26",
          "word": "",
          "sentence": "`अतिथेर्ञ्यः` इति ञ्यप्रत्ययः।"
        }
      ]
    },
    {
      "loc": "1.59",
      "v": "अथाथर्वनिधेस्तस्य विजितारिपुरः पुरः । अर्थ्यामर्थपतिर्वाचमाददे वदतां वरः ॥",
      "sutra_sentences": [
        {
          "sutra": "2.3.41",
          "word": "",
          "sentence": "`यतश्चनिर्धारणम्` इति षष्टी।"
        },
        {
          "sutra": "4.4.92",
          "word": "",
          "sentence": "`धर्मपथ्यर्थन्यायादनपेते` इति यप्रत्ययः।"
        }
      ]
    },
    {
      "loc": "1.60",
      "v": "उपपन्नं ननु शिवं सप्तस्वङ्गेषु यस्य मे । दैवीनां मानुषीणां च प्रतिहर्ता त्वमापदाम् ॥",
      "sutra_sentences": [
        {
          "sutra": "4.3.74",
          "word": "",
          "sentence": "उभयत्रापि `तत आगतः` इत्यण्।"
        },
        {
          "sutra": "4.1.15",
          "word": "",
          "sentence": "`टिड्ढाणञ्-` इत्यादिना ङीप्।"
        }
      ]
    },
    {
      "loc": "1.61",
      "v": "तव मन्त्रकृतो मन्त्रैर्दूरात्प्रशमितारिभिः । प्रत्यादिश्यन्त इव मे दृष्टलक्ष्यभिदः शराः ॥",
      "sutra_sentences": [
        {
          "sutra": "3.2.89",
          "word": "",
          "sentence": "`सुकर्मपापमन्त्रपुण्येषु कृञः` इति क्विप्।"
        }
      ]
    },
    {
      "loc": "1.62",
      "v": "हविरावर्जितं होतस्त्वया विधिवदग्निषु । वृष्टिर्भवति सस्यानामवग्रहविशोषिणाम् ॥",
      "sutra_sentences": [
        {
          "sutra": "3.3.51",
          "word": "",
          "sentence": "`अवे ग्रहो वर्षप्रतिबन्धे` इत्यञ्प्रत्ययः।"
        }
      ]
    },
    {
      "loc": "1.63",
      "v": "पुरुषायुषजीविन्यो निरातङ्का निरीतयः । यन्मदीयाः प्रजास्तस्य हेतुस्त्वद्बह्मवर्चसम् ॥",
      "sutra_sentences": [
        {
          "sutra": "5.4.77",
          "word": "",
          "sentence": "`अचतुर-` आदिसूत्रेणाच्प्रत्ययान्तो निपातः।"
        },
        {
          "sutra": "5.4.78",
          "word": "",
          "sentence": "`ब्रह्महस्तिभ्यां वर्चसः` इत्यच्प्रत्ययः।"
        }
      ]
    },
    {
      "loc": "1.65",
      "v": "किंतु वध्वां तवैतस्यामदृष्टसदृशप्रजम् । न मामवति सद्वीपा रत्नसूरपि मेदिनी ॥",
      "sutra_sentences": [
        {
          "sutra": "3.2.61",
          "word": "",
          "sentence": "`सत्सूद्विष-` इत्यादिना क्विप्।"
        }
      ]
    },
    {
      "loc": "1.66",
      "v": "नूनं मत्तः परं वंश्याः पिण्डविच्छेददर्शिनः । न प्रकामभुजः श्राद्धे स्वधासंग्रहतत्पराः ॥",
      "sutra_sentences": [
        {
          "sutra": "5.3.7",
          "word": "",
          "sentence": "`पञ्चम्यास्तसिल्` ।"
        }
      ]
    },
    {
      "loc": "1.67",
      "v": "मत्परं दुर्लभं मत्वा नूनमावर्जितं मया । पयः पूर्वैः स्वनिःश्वासैः कवोष्णमुपभुज्यते ॥",
      "sutra_sentences": [
        {
          "sutra": "2.3.29",
          "word": "",
          "sentence": "`अन्यारात्-` इत्यादिना पञ्चमी।"
        }
      ]
    },
    {
      "loc": "1.68",
      "v": "सोऽहमिज्याविशुद्धात्मा प्रजालोपनिमीलितः । प्रकाशश्चाप्रकाशश्च लोकालोक इवाचलः ॥",
      "sutra_sentences": [
        {
          "sutra": "3.3.98",
          "word": "",
          "sentence": "`व्रजयजोर्भावे क्यप्` इति क्यप्प्रत्ययः।"
        }
      ]
    },
    {
      "loc": "1.71",
      "v": "असह्यपीडं भगवन्नृणमन्त्यमवेहि मे । अरुन्तुदमिवालानमनिर्वाणस्य दन्तिनः ॥",
      "sutra_sentences": [
        {
          "sutra": "3.2.35",
          "word": "",
          "sentence": "`विध्वरुषोस्तुदः` इति खश्प्रत्ययः।"
        },
        {
          "sutra": "6.3.67",
          "word": "",
          "sentence": "`अरुर्द्विषत्-` इत्यादिना मुमागमः।"
        }
      ]
    },
    {
      "loc": "1.74",
      "v": "सोऽपश्यत्प्रणिधानेन संततेः स्तम्भकारणम् । भावितात्मा भुवो भर्तुरथैनं प्रत्यबोधयत् ॥",
      "sutra_sentences": [
        {
          "sutra": "1.4.52",
          "word": "",
          "sentence": "एनमिति `गतिबुद्धि-` इत्यादिनाणि कर्तुः कर्मत्वम् ॥"
        }
      ]
    },
    {
      "loc": "1.80",
      "v": "हविषे दीर्घसत्रस्य सा चेदानीं प्रचेतसः । भुजङ्गपिहितद्वारं पातालमधितिष्टति ॥",
      "sutra_sentences": [
        {
          "sutra": "1.4.46",
          "word": "",
          "sentence": "`अधिशीङ्स्थासां कर्म` इति कर्मत्वम् ॥"
        }
      ]
    },
    {
      "loc": "1.81",
      "v": "सुतां तदीयां सुरभेः कृत्वा प्रतिनिधिं शुचिः । आराधय सपत्नीकः प्रीता कामदुघा हि सा ॥",
      "sutra_sentences": [
        {
          "sutra": "5.4.153",
          "word": "",
          "sentence": "`नद्यृतश्च` इति कप्प्रत्ययः।"
        },
        {
          "sutra": "3.2.70",
          "word": "",
          "sentence": "सा प्रीतातुष्टा सती। कामान् दोग्धीति कामदुघा भवति। `दुहः कब्धश्च` इति कप्प्रत्ययः।"
        }
      ]
    },
    {
      "loc": "1.82",
      "v": "इति वादिन एवास्य होतुराहुतिसाधनम् । अनिन्द्या नन्दिनी नाम धेनुराववृते वनात् ॥",
      "sutra_sentences": [
        {
          "sutra": "3.3.135",
          "word": "",
          "sentence": "`तृन्` इति तृन्प्रत्ययः।"
        }
      ]
    },
    {
      "loc": "1.83",
      "v": "ललाटोदयमाभुग्नं पल्लवस्निग्धापाटला । बिभ्रती श्वेतरोमाङ्कं संध्येव शशिनं नवम् ॥",
      "sutra_sentences": [
        {
          "sutra": "8.2.45",
          "word": "",
          "sentence": "`ओदितश्च` इति निष्ठातस्य नत्वम्।"
        }
      ]
    },
    {
      "loc": "1.84",
      "v": "भुवं कोष्णेन कुण्डोध्नी मेध्येनावभृथादपि । प्रस्रवेणाभिवर्षन्ती वत्सालोकप्रवर्तिना ॥",
      "sutra_sentences": [
        {
          "sutra": "6.3.107",
          "word": "",
          "sentence": "`कवं चोष्णे` इति चकारात्कादेशः।"
        },
        {
          "sutra": "2.3.35",
          "word": "",
          "sentence": "`बहुर्वीहेरूधसो ङीष्` इति ङीष्॥"
        }
      ]
    },
    {
      "loc": "1.85",
      "v": "रजःकणैः खुरोद्धूतैः स्पृशद्भिर्गात्रमन्तिकात् । तीर्थाभिषेकजां शुद्धिमादधाना महीक्षितः ॥",
      "sutra_sentences": [
        {
          "sutra": "2.3.35",
          "word": "",
          "sentence": "`दूरान्तिकार्थेभ्यो द्वितीया च` इति चकारात्पञ्चमी।"
        }
      ]
    },
    {
      "loc": "1.87",
      "v": "अदूरवर्तिनीं सिद्धिं राजन्विगणयात्मनः । उपस्थितेयं कल्याणी नाम्नि कीर्तित एव यत् ॥",
      "sutra_sentences": [
        {
          "sutra": "4.1.45",
          "word": "",
          "sentence": "`बह्वादिभ्यः` इति ङीप्।"
        }
      ]
    },
    {
      "loc": "1.89",
      "v": "प्रस्थितायां प्रतिष्ठेथाः स्थितायां स्थितिमाचरेः । निषण्णायां निषीदास्यां पीताम्भसि पिबेरपः ॥",
      "sutra_sentences": [
        {
          "sutra": "1.3.22",
          "word": "",
          "sentence": "`समवप्रविभ्यः स्थः` इत्यात्मनेपदम्।"
        }
      ]
    }
  ]
}